{
  "extends": "astro/tsconfigs/strictest",
  "baseUrl": ".",
  "paths": {
    "@components/*": ["src/components/*"],
    "@layouts/*": ["src/layouts/*"]
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "types": ["unplugin-icons/types/astro", "unplugin-icons/types/preact"]
  }
}
